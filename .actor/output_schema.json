{
  "actorOutputSchemaVersion": 1,
  "title": "Lead Data Output",
  "type": "object",
  "properties": {
    "lead_id": {
      "title": "Lead ID",
      "type": "string",
      "template": "string"
    },
    "source": {
      "title": "Source",
      "type": "string",
      "template": "string"
    },
    "profile_url": {
      "title": "Profile URL",
      "type": "string",
      "template": "url"
    },
    "lead_score": {
      "title": "Lead Score",
      "type": "number",
      "template": "number",
      "minimum": 1,
      "maximum": 10
    },
    "scraped_at": {
      "title": "Scraped At",
      "type": "string",
      "format": "date-time",
      "template": "dateTime"
    }
  },
  "required": ["lead_id", "source", "profile_url", "lead_score", "scraped_at"]
}
