    "lead_id": {
      "type": "string",
      "title": "Lead ID",
      "description": "Unique identifier for the lead"
    },
    "source": {
      "type": "string",
      "title": "Source Platform",
      "description": "Platform where the lead was found"
    },
    "name": {
      "type": "string",
      "title": "Name",
      "description": "Full name or display name"
    },
    "username": {
      "type": "string",
      "title": "Username",
      "description": "Platform username"
    },
    "profile_url": {
      "type": "string",
      "title": "Profile URL",
      "description": "URL to the profile page"
    },
    "email": {
      "type": "string",
      "title": "Email",
      "description": "Email address (when available)"
    },
    "bio": {
      "type": "string",
      "title": "Bio",
